####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(OBJCOMP) $(LIBS)

mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: exMoc.pro  /usr/lib/qt-3.3/mkspecs/default/qmake.conf /usr/lib/qt-3.3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile exMoc.pro
qmake: 
	@$(QMAKE) -o Makefile exMoc.pro

dist: 
	@mkdir -p .tmp/exMoc && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/exMoc/ && ( cd `dirname .tmp/exMoc` && $(TAR) exMoc.tar exMoc && $(GZIP) exMoc.tar ) && $(MOVE) `dirname .tmp/exMoc`/exMoc.tar.gz . && $(DEL_FILE) -r .tmp/exMoc

